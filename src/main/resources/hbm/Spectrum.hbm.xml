<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.Spectrum" table="Spectrum"
                    extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup" proxy="uk.ac.ebi.jmzml.model.mzml.Spectrum">
         <!--extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup">-->


        <many-to-one name="scanList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ScanList" column="scanList_hid"/>
        <many-to-one name="precursorList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.PrecursorList" column="precursorList_hid"/>
        <many-to-one name="productList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ProductList" column="productList_hid"/>
        <many-to-one name="binaryDataArrayList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.BinaryDataArrayList" column="binaryDataArrayList_hid"/>

        <property name="id" type="string"/>
        <property name="spotID" type="string"/>
        <property name="index" column="index_order" type="big_integer"/>
        <property name="defaultArrayLength" type="integer"/>

        <property name="dataProcessingRef" type="string"/>
        <many-to-one name="dataProcessing" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.DataProcessing" column="DataProcessing_hid" access="field"/>
        <property name="sourceFileRef" type="string"/>
        <many-to-one name="sourceFile" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SourceFile" column="SourceFile_hid" access="field"/>
    </union-subclass>
</hibernate-mapping>
