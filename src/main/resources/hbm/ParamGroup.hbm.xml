<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.ParamGroup" table="ParamGroup" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <list name="referenceableParamGroupRef" access="field" cascade="all">
            <key column="ParamGroup_hid"/>
            <index column="idx"/>
            <many-to-many column="ReferenceableParamGroupRef_hid"
                          class="uk.ac.ebi.jmzml.model.mzml.ReferenceableParamGroupRef"/>
        </list>
        <list name="cvParam" access="field" cascade="all" table="ParamGroup_CVParam">
            <key column="ParamGroup_hid"/>
            <index column="idx"/>
            <many-to-many column="CVParam_hid" class="uk.ac.ebi.jmzml.model.mzml.CVParam"/>
        </list>
        <list name="userParam" access="field" cascade="all" table="ParamGroup_UserParam">
            <key column="ParamGroup_hid"/>
            <index column="idx"/>
            <many-to-many column="UserParam_hid" class="uk.ac.ebi.jmzml.model.mzml.UserParam"/>
        </list>
    </union-subclass>
</hibernate-mapping>