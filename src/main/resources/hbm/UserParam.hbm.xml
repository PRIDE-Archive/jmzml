<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="property">
    <class name="uk.ac.ebi.jmzml.model.mzml.UserParam" table="UserParam">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="name" type="string" not-null="true"/>
        <property name="type" type="string"/>
        <property name="value" type="string"/>
        <property name="unitAccession" type="string"/>
        <property name="unitName" type="string"/>

        <many-to-one name="unitCV" class="uk.ac.ebi.jmzml.model.mzml.CV" cascade="all"/>
    </class>
</hibernate-mapping>