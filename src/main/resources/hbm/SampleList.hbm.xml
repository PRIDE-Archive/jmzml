<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.SampleList" table="SampleList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="big_integer"/>
        <list name="sample" access="field" cascade="all" table="SampleList_Sample">
            <key column="SampleList_hid"/>
            <index column="idx"/>
            <many-to-many column="Sample_hid" class="uk.ac.ebi.jmzml.model.mzml.Sample"/>
        </list>
    </union-subclass>
</hibernate-mapping>