<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzml.model.mzml.IndexedmzML" table="IndexedmzML">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <one-to-one name="mzML" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.MzML"/>
        <one-to-one name="indexList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.IndexList"/>
        <property name="indexListOffset" type="long"/>
        <property name="fileChecksum" type="string"/>
    </class>
</hibernate-mapping>