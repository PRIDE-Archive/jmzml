<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.ScanSettings"  table="ScanSettings"  extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup">
        <one-to-one name="sourceFileRefList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SourceFileRefList"/>
        <one-to-one name="targetList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.TargetList"/>
        <property name="id" type="string"/>
    </union-subclass>
</hibernate-mapping>
