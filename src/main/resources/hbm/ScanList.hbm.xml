<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.ScanList" table="ScanList"
                    extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup">
        <property name="count" type="big_integer"/>
        <list name="scan" access="field" cascade="all" table="ScanList_Scan">
            <key column="ScanList_hid"/>
            <index column="idx"/>
            <many-to-many column="Scan_hid" class="uk.ac.ebi.jmzml.model.mzml.Scan"/>
        </list>
    </union-subclass>
</hibernate-mapping>
