<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.ac.ebi.jmzml.model.mzml">
    <union-subclass name="InstrumentConfigurationList" table="InstrumentConfigurationList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="big_integer"/>
        <list name="instrumentConfiguration" access="field" cascade="all">
            <key column="InstrumentConfigList_hid"/>
            <index column="idx"/>
            <many-to-many column="InstrumentConfiguration_hid"
                          class="InstrumentConfiguration"/>
        </list>
    </union-subclass>
</hibernate-mapping>