<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzml.model.mzml.SourceFileList" table="SourceFileList">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="count" type="big_integer"/>
        <list name="sourceFile" access="field" cascade="all" table="SourceFileList_SourceFile">
            <key column="SourceFileList_hid"/>
            <index column="idx"/>
            <many-to-many column="SourceFile_hid" class="uk.ac.ebi.jmzml.model.mzml.SourceFile"/>
        </list>
    </class>
</hibernate-mapping>