<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.ac.ebi.jmzml.model.mzml" default-access="property">
    <class name="Offset" table="Offset">
        <id name="hid" type="long">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>

        <property name="value" column="value" type="long"/>
        <property name="idRef" column="idRef" type="string" />
        <property name="spotID" column="spotID" type="string" />
        <property name="scanTime" column="scanTime" type="double" />
    </class>
</hibernate-mapping>