<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.InstrumentConfiguration" table="InstrumentConfiguration"
                    extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup">
        <many-to-one name="componentList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ComponentList" column="ComponentList_hid"/>
        <many-to-one name="software" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.Software" column="Software_hid" access="field"/>
        <property name="softwareRef" type="string"/>
        <property name="id" type="string"/>
        <property name="scanSettingsRef" type="string"/>
        <many-to-one name="scanSettings" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ScanSettings" column="ScanSettings_hid" access="field"/>
    </union-subclass>
</hibernate-mapping>
