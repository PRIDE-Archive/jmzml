<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzml.model.mzml.DataProcessing" table="DataProcessing">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="id" type="string"/>
        <list name="processingMethod" access="field" cascade="all" table="DataProcessing_ProcessingMethod">
                <key column="DataProcessing_hid"/>
                <index column="idx"/>
                <many-to-many column="ProcessingMethod_hid" class="uk.ac.ebi.jmzml.model.mzml.ProcessingMethod"/>
        </list>
    </class>
</hibernate-mapping>