<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.DataProcessing" table="DataProcessing" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="id" type="string"/>
        <list name="processingMethod" access="field" cascade="all" >
            <key column="DataProcessing_hid"/>
            <index column="idx"/>
            <many-to-many column="ProcessingMethod_hid" class="uk.ac.ebi.jmzml.model.mzml.ProcessingMethod"/>
        </list>
    </union-subclass>
</hibernate-mapping>