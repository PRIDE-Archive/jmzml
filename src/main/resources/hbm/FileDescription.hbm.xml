<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzml.model.mzml.FileDescription" table="FileDescription">
        <id name="hid" type="java.lang.Long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <many-to-one name="fileContent" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ParamGroup"/>
        <many-to-one name="sourceFileList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SourceFileList"/>
        <list name="contact" access="field" cascade="all" table="FileDescription_ParamGroup">
                <key column="FileDescription_hid"/>
                <index column="idx"/>
                <many-to-many column="ParamGroup_hid" class="uk.ac.ebi.jmzml.model.mzml.ParamGroup"/>
        </list>
    </class>
</hibernate-mapping>