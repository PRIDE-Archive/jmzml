<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.ScanSettingsList" table="ScanSettingsList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject" proxy="uk.ac.ebi.jmzml.model.mzml.ScanSettingsList">
        <property name="count" type="big_integer"/>
        <list name="scanSettings" access="field" cascade="all" table="ScanSettingsList_ScanSettings">
            <key column="ScanSettingsList_hid"/>
            <index column="idx"/>
            <many-to-many column="ScanSettings_hid" class="uk.ac.ebi.jmzml.model.mzml.ScanSettings"/>
        </list>
    </union-subclass>
</hibernate-mapping>