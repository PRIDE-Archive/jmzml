<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="property">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.SpectrumList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
       <list name="spectrum" access="field" cascade="all" table="SpectrumList_Spectrum">
            <key column="SpectrumList_hid"/>
            <index column="idx"/>
            <many-to-many column="Spectrum_hid" class="uk.ac.ebi.jmzml.model.mzml.Spectrum"/>
        </list>

        <property name="count" type="big_integer"/>

        <property name="defaultDataProcessingRef" type="string"/>
        <!--<many-to-one name="defaultDataProcessing" class="uk.ac.ebi.jmzml.model.mzml.DataProcessing" cascade="all" column="Default_DataProcessing_hid"/>-->
    </union-subclass>
</hibernate-mapping>