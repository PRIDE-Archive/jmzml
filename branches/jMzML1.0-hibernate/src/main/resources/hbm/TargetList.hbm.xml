<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="property">
    <class name="uk.ac.ebi.jmzml.model.mzml.TargetList" table="TargetList">

        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>

        <property name="count" type="big_integer" not-null="true"/>

        <list name="target" access="field" cascade="all" table="TargetList_ParamGroup">
            <key column="TargetList_hid"/>
            <index column="idx"/>
            <many-to-many class="uk.ac.ebi.jmzml.model.mzml.ParamGroup" column="ParamGroup_hid"/>
        </list>
    </class>
</hibernate-mapping>