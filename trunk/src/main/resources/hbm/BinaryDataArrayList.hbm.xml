<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.BinaryDataArrayList" table="BinaryDataArrayList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="big_integer"/>
        <list name="binaryDataArray" access="field" cascade="all">
            <key column="BinaryDataArrayList_hid"/>
            <index column="idx"/>
            <many-to-many column="BinaryDataArray_hid" class="uk.ac.ebi.jmzml.model.mzml.BinaryDataArray"/>
        </list>
    </union-subclass>
</hibernate-mapping>