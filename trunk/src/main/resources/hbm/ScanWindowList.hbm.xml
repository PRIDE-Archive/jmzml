<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.ScanWindowList" table="ScanWindowList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="integer"/>
        <list name="scanWindow" access="field" cascade="all" table="ScanWindow_ParamGroup">
            <key column="ScanWindow_hid"/>
            <index column="idx"/>
            <many-to-many column="ParamGroup_hid" class="uk.ac.ebi.jmzml.model.mzml.ParamGroup"/>
        </list>
    </union-subclass>
</hibernate-mapping>