<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="uk.ac.ebi.jmzml.model.mzml">
    <class name="SelectedIonList" table="SelectedIonList">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="count" type="big_integer"/>
        <list name="selectedIon" access="field" cascade="all" table="SelectedIonList_ParamGroup">
                <key column="SelectedIonList_hid"/>
                <index column="idx"/>
                <many-to-many column="ParamGroup_hid" class="ParamGroup"/>
        </list>
    </class>
</hibernate-mapping>