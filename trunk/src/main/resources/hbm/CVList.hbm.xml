<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.CVList" table="CVList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="big_integer"/>
        <list name="cv" access="field" cascade="all" table="CVList_CV">
            <key column="CVList_hid"/>
            <index column="idx"/>
            <many-to-many column="CV_hid" class="uk.ac.ebi.jmzml.model.mzml.CV"/>
        </list>
    </union-subclass>
</hibernate-mapping>