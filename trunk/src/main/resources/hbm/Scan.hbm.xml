<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.Scan" table="Scan" extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup">

        <list name="scanWindowList" access="field" cascade="all" table="ScanWindowList">
            <key column="SW_hid"/>
            <index column="SW_idx"/>
            <one-to-many class="uk.ac.ebi.jmzml.model.mzml.ParamGroup"/>
        </list>


        <property name="spectrumRef" type="string"/>
        <property name="sourceFileRef" type="string"/>
        <!--<many-to-one name="sourceFile" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SourceFile" column="SourceFile_hid"/>-->
        <property name="externalSpectrumID" type="string"/>
        <property name="instrumentConfigurationRef" type="string"/>
        <!--<many-to-one name="instrumentConfiguration" cascade="all"-->
                     <!--class="uk.ac.ebi.jmzml.model.mzml.InstrumentConfiguration" column="InstrumentConfiguration_hid"/>-->
    </union-subclass>
</hibernate-mapping>
