<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.Run" table="Run" extends="uk.ac.ebi.jmzml.model.mzml.ParamGroup" proxy="uk.ac.ebi.jmzml.model.mzml.Run">
        <many-to-one name="spectrumList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SpectrumList" column="SpectrumList_hid"/>
        <many-to-one name="chromatogramList" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.ChromatogramList" column="ChromatogramList_hid"/>
        <property name="id" type="string"/>
        <property name="defaultInstrumentConfigurationRef" type="string"/>
        <many-to-one name="defaultInstrumentConfiguration" cascade="all"
                     class="uk.ac.ebi.jmzml.model.mzml.InstrumentConfiguration" column="InstrumentConfiguration_hid" access="field"/>
        <property name="defaultSourceFileRef" type="string"/>
        <many-to-one name="defaultSourceFile" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.SourceFile" column="Default_SourceFile_hid" access="field"/>
        <property name="sampleRef" type="string"/>
        <many-to-one name="sample" cascade="all" class="uk.ac.ebi.jmzml.model.mzml.Sample" column="Sample_hid" access="field"/>
        <property name="startTimeStamp" type="calendar"/>
    </union-subclass>
</hibernate-mapping>
