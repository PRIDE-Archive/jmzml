<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="property" package="uk.ac.ebi.jmzml.model.mzml">
    <class name="UserParam" table="UserParam">

        <id name="hid" column="hid" type="long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>

        <property name="name" type="string" column="name" not-null="true"/>
        <property name="type" type="string" column="type" not-null="true"/>
        <property name="value" type="string" column="value" not-null="true"/>
        <property name="unitAccession" type="string" column="unitAccession" not-null="false"/>
        <property name="unitName" type="string" column="unitName" not-null="false"/>

        <many-to-one name="unitCV" column="unitCV" class="CV" cascade="all" not-null="false" />
    </class>
</hibernate-mapping>