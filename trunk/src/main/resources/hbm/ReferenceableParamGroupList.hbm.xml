<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzml.model.mzml.ReferenceableParamGroupList" table="ReferenceableParamGroupList">
        <id name="hid" type="long" access="field">
            <column name="hid"/>
            <generator class="native"/>
        </id>
        <property name="count" type="big_integer"/>
        <list name="referenceableParamGroup" access="field" cascade="all" table="ReferenceableParamGroupList_ReferenceableParamGroup">
                <key column="ReferenceableParamGroupList_hid"/>
                <index column="idx"/>
                <many-to-many column="ReferenceableParamGroup_hid" class="uk.ac.ebi.jmzml.model.mzml.ReferenceableParamGroup"/>
        </list>
    </class>
</hibernate-mapping>