<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzml.model.mzml.SoftwareList" table="SoftwareList" extends="uk.ac.ebi.jmzml.model.mzml.MzMLObject">
        <property name="count" type="big_integer"/>
        <list name="software" access="field" cascade="all" table="SoftwareList_Software">
            <key column="SoftwareList_hid"/>
            <index column="idx"/>
            <many-to-many column="Software_hid" class="uk.ac.ebi.jmzml.model.mzml.Software"/>
        </list>
    </union-subclass>
</hibernate-mapping>